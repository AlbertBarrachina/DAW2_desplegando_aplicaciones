package proyecto;

public class muebles {

	private String color;
	private int altura, ancho, peso;
	
	
	//getters y setters
	//color
	public String getColor() {
		return color;
	}
	
	public void setColor(String color) {
		this.color = color;
	}
	
	//altura
	public int getAltura() {
		return altura;
	}
	
	public void setAltura(int altura) {
		this.altura = altura;
	}
	
	//ancho
	public int getAncho() {
		return ancho;
	}
	
	public void setAncho(int ancho) {
		this.ancho = ancho;
	}
	
	//peso
	public int getPeso() {
		return peso;
	}
	
	public void setColor(int peso) {
		this.peso = peso;
	}
	
}
